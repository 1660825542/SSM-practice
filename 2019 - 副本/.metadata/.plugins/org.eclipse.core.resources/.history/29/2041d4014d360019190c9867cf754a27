package first;

import java.io.File;
import java.io.FileInputStream;
import java.io.FileNotFoundException;
import java.io.FileOutputStream;
import java.io.IOException;
import java.util.HashMap;
import java.util.Map;
import java.util.Scanner;
import java.util.Set;

public class Q {
	public static void main(String[] args) {
		
	}

	private static final boolean String = false;
	private static Map<String, String> m=new HashMap<String, String>();
	public static Scanner sc=new Scanner(System.in);
	
	static{
		try {
			FileInputStream in=new FileInputStream(new File("D:/123.txt"));
			FileOutputStream out=new FileOutputStream(new File("D:/123.txt"));
			while(true){
			System.out.print("c£∫");
			int x=sc.nextInt();
			
			switch (x) {
			case 1:
				System.out.print("–’√˚£∫");
				String s=sc.next();
				System.out.print("ƒÍ¡‰£∫");
				String a=sc.next();
				m.put(s, a);
				break;
			case 2:
				Set<String> k=m.keySet();
				String num="";
				for(String s1:k)
				{
					num=num+s1+":"+m.get(s1)+"\r\n";
				}
				byte [] b=num.getBytes();
				out.write(b);
				out.flush();
				out.close();
				break;
			case 3:
				String ss="";
				int a1=in.read();
				while(a1!=-1)
				{
					//System.out.print((char)a1);
					ss+=(char)a1;
					a1=in.read();
				}
				System.out.println(ss+"333");
				break;
			default:
				break;
				
		}
			}
		} catch (FileNotFoundException e) {
			
		} catch (IOException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	}
	
}
